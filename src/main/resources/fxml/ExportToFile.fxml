<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="anchorPane" prefHeight="206.0" prefWidth="504.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.sub_ui.controller.ExportToFile">
    <TextField fx:id="fileName" layoutX="17.0" layoutY="95.0" prefHeight="34.0" prefWidth="475.0"
               promptText="Tên file xuất...">
        <font>
            <Font size="14.0"/>
        </font>
    </TextField>
    <Button fx:id="browseButton" layoutX="17.0" layoutY="148.0" onAction="#chooseDir" prefHeight="34.0" prefWidth="85.0"
            text="Browse...">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Label fx:id="dirLabel" layoutX="109.5" layoutY="148.0" prefHeight="34.0" prefWidth="302.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Button layoutX="419.0" layoutY="148.0" onAction="#submitExport" prefHeight="34.0" prefWidth="73.0" text="Export">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Label alignment="CENTER" layoutX="17.0" layoutY="14.0" prefHeight="34.0" prefWidth="475.0"
           text="Nhập tên file xuất và chọn thư mục để lưu file ">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label alignment="CENTER" layoutX="17.0" layoutY="48.0" prefHeight="34.0" prefWidth="475.0"
           text="Mỗi dòng gồm: &quot;từ&quot;, dấu TAB và  &quot;nghĩa tương ứng&quot;">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
</AnchorPane>
